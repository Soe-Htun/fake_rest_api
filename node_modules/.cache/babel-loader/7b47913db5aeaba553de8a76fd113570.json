rop=\"address\" label=\"address\" min-width=\"180\" sortable> </el-table-column>\n             <el-table-column label=\"Operation\">\n                <template slot=\"scope\">\n                    <!-- <El-button size = \"small\" @ click = \"handleEdit (. Scope $ index, scope.row)\"> Edit </ el-button>\n                    <El-button type = \"danger\" size = \"small\" @ click = \"handleDel (scope $ index, scope.row.)\"> Delete </ el-button> -->\n                    <el-button size=\"small\" @ click=\"handleEdit (scope.row)\"> Edit </el-button>\n                    <el-button type=\"danger\" size=\"small\" @ click=\"handleDel (scope.row)\"> Delete </el-button>\n                </template>\n            </el-table-column>\n    </el-table>\n\n         <!-- Table by value -->\n    <!-- <List :message='byValue'></List> -->\n\n         <!-- paging paging -->\n    <el-pagination \n    @size-change=\"handleSizeChange\"\n    @current-change=\"handleCurrentChange\"\n    :current-page=\"currentPage\"\n    :page-size=\"pageSize\" \n    layout=\"total, prev, pager, next\"\n    background\n    :total=\"total\" \n    ></el-pagination>\n    </div>\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            id:'',\n            name:'',\n            sex:'',\n            age:'',\n            birthday:'',\n            address:'',\n\n            showData:[\n                {\n                    id:\"1\",\n                    name:\"Paysaung\",\n                    sex:'Male',\n                    age:22,\n                    birthday:\"14-04-1998\",\n                    address:\"Salin\"\n                },\n                {\n                    id:\"2\",\n                    name:\"Paysaung\",\n                    sex:'Male',\n                    age:22,\n                    birthday:\"14-04-1998\",\n                    address:\"Salin\"\n                },\n                {\n                    id:\"3\",\n                    name:\"Paysaung\",\n          